{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"build": "tsc && biome check ./src && cspell ./src && vite build",
		"lint": "biome check ./src",
		"format": "biome format --write ./src",
		"check": "tsc --noEmit && biome ci ./src && cspell ./src",
		"cspell": "cspell ./src",
		"preview": "vite preview --host"
	},
	"dependencies": {
		"@mantine/core": "^8.1.1",
		"@mantine/notifications": "^8.1.1",
		"@react-spring/web": "^10.0.1",
		"@tabler/icons-react": "^3.34.0",
		"ky": "^1.8.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.6.2",
		"remark-gfm": "^4.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"cspell": "^9.1.1",
		"globals": "^16.0.0",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5"
	}
}
